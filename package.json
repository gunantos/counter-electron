{
  "name": "counter",
  "version": "1.0.0",
  "description": "Aplikasi antrian mengunakan Eelctron, aplikasi ini menyimpan data di Store Chrome",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "build:release": "electron-builder -p always",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/gunantos/electron-antrian.git",
  "keywords": [],
  "author": {
    "name": "Gunanto Simamora",
    "email": "gunanto.simamora@gmail.com",
    "url": "https://app-kita.com"
  },
  "devDependencies": {
    "electron": "^9.1.0",
    "electron-builder": "^22.9.1",
    "@electron-forge/publisher-github": "^6.0.0-beta.56"
  },
  "dependencies": {
    "electron-pos-printer": "^1.2.0",
    "electron-updater": "^4.3.9"
  },
  "config": {
    "forge": {
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "gunantos",
              "name": "electron-antrian"
            },
            "draft": true
          }
        }
      ]
    }
  }
}
